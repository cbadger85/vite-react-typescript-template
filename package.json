    "@types/node": "^16.11.26",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react": "^1.3.0",
    "@vitest/ui": "^0.9.3",
    "c8": "^7.11.0",
    "eslint": "^8.13.0",
