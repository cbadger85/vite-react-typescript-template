{
  "name": "vite-starter",
  "version": "0.0.1",
  "description": "A starter setup for Vite, React and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "dev:clean": "npm run dev -- --force",
